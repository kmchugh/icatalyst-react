{
  "name": "@icatalyst/layouts",
  "version": "0.0.1",
  "description": "complex elements for the @icatalyst/react mvp framework",
  "author": "ICatalyst Pte. Ltd.",
  "homepage": "https://github.com/kmchugh/icatalyst-react#readme",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "module": "src/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "es",
    "esm",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmchugh/icatalyst-react.git"
  },
  "scripts": {
    "clean": "rimraf lib",
    "prepack": "yarn build",
    "build": "yarn clean && yarn compile",
    "compile": "tsc --build && cp \"./package.json\" ./lib/",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0"
  },
  "bugs": {
    "url": "https://github.com/kmchugh/icatalyst-react/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@icatalyst/core": "^0.0.1",
    "@icatalyst/components": "^0.0.1"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/public/build"
        ]
      }
    }
  }
}