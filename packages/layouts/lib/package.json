{
  "name": "@icatalyst/layouts",
  "version": "0.0.1",
  "description": "complex elements for the @icatalyst/react mvp framework",
  "author": "ICatalyst Pte. Ltd.",
  "homepage": "https://github.com/kmchugh/icatalyst-react#readme",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "module": "src/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmchugh/icatalyst-react.git"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "yarn clean && yarn compile",
    "compile": "tsc --build && cp \"./package.json\" ./lib/",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0"
  },
  "bugs": {
    "url": "https://github.com/kmchugh/icatalyst-react/issues"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "dependencies": {
    "@icatalyst/core": "^0.0.1",
    "@icatalyst/components": "^0.0.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@mui/styles": "^5.9.0",
    "@mui/types": "^7.1.4",
    "@material-ui/core": "^4.12.4",
    "material-ui-color": "^1.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^8.0.2"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/public/build"
        ]
      }
    }
  }
}